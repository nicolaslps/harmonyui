<meta property="og:type" content="website">
<meta property="og:url" content="{{ app.request.uri }}">
<meta property="og:title" content="{{ seo_title_content }}">
<meta property="og:description" content="{{ seo_description_content }}">
<meta property="og:image" content="{{ ogImage }}">
<meta property="og:image:alt" content="{{ seo_title_content }}">
<meta property="og:site_name" content="HarmonyUI">
<meta property="og:locale" content="{{ app.locale|replace({_: '_'})|default('en_US') }}">
